<html>
  <head>
    <link href="./css/styles.css" rel="stylesheet" />
  </head>
  <body>
    <%- include('header.ejs') %>
    <table>
      <tr>
        <th>Task</th>
        <th>Completed</th>
      </tr>
      <% for(var i=0; i<allTodos.length; i++) { %>
      <tr>
        <td><%= allTodos[i].title %></td>
        <td><%= allTodos[i].completed %></td>
      </tr>
      <% } %>
    </table>

  </body>
</html>
